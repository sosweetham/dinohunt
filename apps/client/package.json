{
    "name": "@dinohunt/client",
    "version": "0.0.1",
    "devDependencies": {
        "@lucide/svelte": "^0.485.0",
        "@playwright/test": "^1.49.1",
        "@sveltejs/adapter-static": "^3.0.8",
        "@sveltejs/kit": "^2.16.0",
        "@sveltejs/vite-plugin-svelte": "^5.0.0",
        "@tailwindcss/container-queries": "^0.1.1",
        "@tailwindcss/forms": "^0.5.10",
        "@tailwindcss/typography": "^0.5.16",
        "autoprefixer": "^10.4.20",
        "bits-ui": "^1.4.3",
        "clsx": "^2.1.1",
        "formsnap": "1.0.1",
        "lucide-svelte": "^0.485.0",
        "mdsvex": "^0.12.3",
        "mode-watcher": "^0.5.1",
        "prettier": "^3.1.0",
        "prettier-plugin-svelte": "^3.1.0",
        "prettier-plugin-tailwindcss": "^0.6.11",
        "svelte": "^5.0.0",
        "svelte-check": "^4.0.0",
        "svelte-sonner": "^0.3.28",
        "svelte-turnstile": "^0.10.0",
        "sveltekit-superforms": "^2.24.0",
        "tailwind-merge": "^3.0.2",
        "tailwind-variants": "^1.0.0",
        "tailwindcss": "^3.4.17",
        "tailwindcss-animate": "^1.0.7",
        "typescript": "^5.0.0",
        "vite": "^6.0.0",
        "vitest": "^3.0.0",
        "zod": "^3.24.2"
    },
    "private": true,
    "scripts": {
        "dev": "vite dev",
        "build": "vite build",
        "preview": "vite preview",
        "prepare": "svelte-kit sync || echo ''",
        "check-fix": "bunx @biomejs/biome check --write ./src",
        "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
        "test:unit": "vitest",
        "test": "npm run test:unit -- --run && npm run test:e2e",
        "test:e2e": "playwright test",
        "format": "npx @biomejs/biome format --write ./src",
        "format-svelte": "npx prettier --write ./src",
        "check-format": "npx @biomejs/biome format ./src",
        "lint": "npx @biomejs/biome lint --write ./src",
        "check-lint": "npx @biomejs/biome lint ./src"
    },
    "type": "module",
    "dependencies": {
        "better-auth": "^1.2.6-beta.4",
        "runed": "^0.25.0"
    },
    "prettier": {
        "svelteSortOrder": "options-styles-scripts-markup",
        "svelteStrictMode": true,
        "svelteBracketNewLine": false,
        "svelteAllowShorthand": false,
        "svelteIndentScriptAndStyle": false,
        "overrides": [
            {
                "files": "*.svelte",
                "options": {
                    "parser": "svelte"
                }
            }
        ],
        "plugins": ["prettier-plugin-svelte", "prettier-plugin-tailwindcss"]
    }
}
